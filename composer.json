{
    "name": "riclep/nova-mailchimp-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "spatie/laravel-newsletter": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Riclep\\NovaMailchimpField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Riclep\\NovaMailchimpField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
