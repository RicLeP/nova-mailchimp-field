{
    "name": "riclep/nova-mailchimp-field",
    "description": "A Laravel Nova field for managing Mailchimp subscriptions.",
    "keywords": [
        "laravel",
        "nova",
		"mailchimp"
    ],
	"authors": [
		{
			"name": "Ric Le Poidevin",
			"email": "ric@sirric.co.uk",
			"homepage": "http://sirric.co.uk",
			"role": "Developer"
		}
	],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "spatie/laravel-newsletter": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Riclep\\NovaMailchimpField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Riclep\\NovaMailchimpField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
